#include <stdio.h>
#define n 5
int main() {
int vetor[n] = {3, 5, 1, 2, 4};
int aux, i, j;
for ( i = n-1 ; i > 0 ; i-- ) {
for ( j = 0 ; j < i ; j++ ) {
if ( vetor[j] > vetor[j+1] ) {
aux = vetor[j];
vetor[j] = vetor[j+1];
vetor[j+1] = aux;
}
}
}
printf("Ordenado:\n");
for ( i = 0 ; i < n ; i++ ) {
printf("Posicao %d = %d\n", i, vetor[i]);
}
return 0;
}
